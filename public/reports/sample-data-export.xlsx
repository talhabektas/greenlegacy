        <tr>
          <th>A</th>
          <th>B</th>
          <th>C</th>
          <th>D</th>
        </tr>
        <tr>
          <td><strong>Ürün</strong></td>
          <td><strong>Üretim (ton)</strong></td>
          <td><strong>Verim (ton/ha)</strong></td>
          <td><strong>Kalite Skoru (%)</strong></td>
        </tr>
        <tr>
          <td>Domates (Sera)</td>
          <td>68.2</td>
          <td>76.4</td>
          <td>94.5</td>
        </tr>
        <tr>
          <td>Salatalık (Sera)</td>
          <td>42.5</td>
          <td>52.5</td>
          <td>96.8</td>
        </tr>
        <tr>
          <td>Biber (Sera)</td>
          <td>35.1</td>
          <td>43.8</td>
          <td>92.3</td>
        </tr>
        <tr>
          <td>Portakal</td>
